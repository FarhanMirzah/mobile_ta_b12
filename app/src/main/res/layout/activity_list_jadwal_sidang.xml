<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listjadwal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListJadwalSidangActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="buttonBackListMahasiswa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvJadwal"
        android:layout_width="364dp"
        android:layout_height="380dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/judulLaman"
        app:layout_constraintHorizontal_bias="0.659"
        app:layout_constraintStart_toStartOf="@+id/judulLaman"
        app:layout_constraintTop_toBottomOf="@+id/judulLaman"
        tools:listitem="@layout/item_jadwal" />

    <TextView
        android:id="@+id/judulLaman"
        android:layout_width="358dp"
        android:layout_height="66dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="16dp"
        android:text="List Jadwal Sidang Mahasiswa Bimbingan Anda"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:paddingLeft="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/Seminar"
        android:layout_width="224dp"
        android:layout_height="72dp"
        android:layout_marginBottom="32dp"
        android:onClick="buttonListJadwalSeminar"
        android:text="List Jadwal Seminar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>

